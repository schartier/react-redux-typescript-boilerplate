{
  "name": "react-redux-typescript-boilerplate",
  "version": "1.0.1",
  "private": false,
  "description": "A frontend boilerplate with React, Redux and Typescript",
  "homepage": "https://github.com/schartier/react-redux-typescript-boilerplate",
  "keywords": [
    "react",
    "redux",
    "typescript",
    "boilerplate",
    "jest",
    "eslint",
    "sass"
  ],
  "main": "index.js",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "jest",
    "start": "webpack-dev-server --mode development --hot --progress --color --port 3000 --open",
    "build": "webpack -p --progress --colors",
    "eslint": "eslint '*/**/*.{js,ts}' --quiet --fix",
    "lint": "npm run eslint && tsc --noEmit",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,css,scss}\""
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@types/classnames": "^2.2.7",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.11",
    "@types/node": "^10.12.18",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "@types/react-redux": "^7.0.0",
    "@types/react-router": "^4.4.3",
    "@types/redux-actions": "2.3.1",
    "@types/webpack": "^4.4.23",
    "@types/webpack-env": "1.13.6",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "babel-jest": "^24.3.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "gh-pages": "^2.0.1",
    "html-loader": "^1.0.0-alpha.0",
    "html-webpack-plugin": "^4.0.0-alpha",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.3.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss": "^7.0.13",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "postcss-reporter": "^6.0.1",
    "postcss-url": "^8.0.0",
    "prettier": "^1.16.4",
    "prettier-loader": "^2.1.1",
    "react-hot-loader": "^4.6.3",
    "redux-devtools-extension": "^2.13.7",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.0",
    "ts-loader": "^5.3.3",
    "typescript": "^3.2.4",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.4",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4"
  }
